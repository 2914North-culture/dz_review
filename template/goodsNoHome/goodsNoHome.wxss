/* 详情页goods样式 */
.ct-nohome-list{
  max-width:100%;
  height:200rpx;
  /* background-color:pink; */
  padding:20rpx 0;
  box-sizing:border-box;
  display:flex;
  align-items:center;
}
.ct-nohome-list .ct-nohome-image{
  max-width:204rpx;
  max-height:204rpx;
  border-radius:10rpx;
  border:solid 1px #e7e7e7;
}
.ct-nohome-list .ct-nohome-msg{
  padding-left:30rpx;
}
.ct-nohome-list .ct-nohome-msg .ct-nohome-title{
  font-size:36rpx;
  color:#000;
  font-weight:bold;
}
.ct-nohome-list .ct-nohome-msg .ct-nohome-state{
  color:#000;
  font-size:28rpx;
  display:flex;
  align-items:center;
}
.ct-nohome-list .ct-nohome-msg .ct-nohome-state .ct-state-star{
  width:180rpx;
  height:34rpx;
  background-image: url(data:image/png;base64,);
  background-repeat:no-repeat;
  background-size:cover;
  position:relative;
}
.ct-nohome-list .ct-nohome-msg .ct-nohome-state .ct-state-disstar{
  width:100%;
  height:34rpx;
  background-image: url(data:image/png;base64,);
  background-repeat:no-repeat;
  background-size:cover;
  position:absolute;
  left:0;
  top:0;
}
.ct-nohome-list .ct-nohome-msg .ct-nohome-state .ct-info-all{
  margin:0 15rpx;
}
.ct-nohome-list .ct-nohome-msg .ct-nohome-score{
  display:flex;
  font-size:24rpx;
}
.ct-nohome-list .ct-nohome-msg .ct-nohome-score .ct-score-classify{
  display:flex;
  align-items:center;
}
.ct-nohome-list .ct-nohome-msg .ct-nohome-score .ct-score-classify:nth-child(2){
  margin:0 9rpx;
}

.ct-nohome-list .ct-nohome-msg .ct-nohome-score .ct-score-classify .ct-scored{
  font-size: 18rpx;
}
.ct-nohome-list .ct-nohome-msg .ct-nohome-todo{
  display:flex;
  color:24rpx;
  font-size:24rpx;
}
.ct-nohome-list .ct-nohome-msg .ct-nohome-todo .ct-todo-place{
  margin-right:20rpx;
}